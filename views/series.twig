{% extends "layout/layout.twig" %}

{% block content %}

{{ __("SERIES_LIST") }}

<div>{{ series }}</div>
    <table style="width:100%">
    <tr>
        <th>Title</th>
        <th>Description</th>
    </tr>
        {% for serie in series %}
    <tr>
        <td>{{ serie.title }}</td>
        <td>{{ serie.overview }}</td>
    </tr>
        {% endfor %}
</table> 

{% endblock %}
